    <!-- Core theme JS-->
    <script src="/js/scripts.js"></script>
    <script src="/js/main.js"></script>
    <script src="/js/bootstrap-notify.min.js"></script>
    <script src="/js/notify.js"></script>
    <script src="/js/studentForm.js"></script>

</body>

</html>